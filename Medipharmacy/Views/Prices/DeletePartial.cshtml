@model Medipharmacy.Models.Price

<div class="modal-header">
    <h5 class="modal-title" id="exampleModalLabel">Delete Price</h5>
</div>
<div class="modal-body">
    <h6>Are You Sure You Want To Detete The Price?</h6>
</div>
<div class="modal-footer">
    <button data-bs-dismiss="modal" class="btn btn-warning w-25">Cancel</button>
    <a asp-action="Remove" asp-route-id="@Model.PriceId" class="btn btn-danger w-25">Delete</a>
</div>